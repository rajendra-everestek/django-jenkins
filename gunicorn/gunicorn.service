[Unit]
Description=gunicorn daemon
Requires=gunicorn.socket
After=network.target

[Service]
User=harry
Group=www-data
WorkingDirectory=/var/lib/jenkins/workspace/Django-Admin/djangojenkin
ExecStart=/var/lib/jenkins/workspace/Django-Admin/env/bin/gunicorn \
          --access-logfile - \
          --workers 3 \
          --bind unix:/run/gunicorn.sock \
          djangojenkin.wsgi:application

[Install]
WantedBy=multi-user.target